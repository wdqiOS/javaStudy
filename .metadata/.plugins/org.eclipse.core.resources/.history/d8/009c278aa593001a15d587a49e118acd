package com.lz.Animal;

public class Circle extends Shape {
	private double  r;
	
	public Circle () {}
	public Circle (double r) {
		this.setR(r);
	}
	
	// set/get方法
	public void setR(double r) {
		this.r = r;
	}
	public double getR() {
		return r;
	}
	
	
	@Override
	public double area() {
		
		return 0.0;
	}

}
