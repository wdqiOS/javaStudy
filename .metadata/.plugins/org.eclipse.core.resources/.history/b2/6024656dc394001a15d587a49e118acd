package wrap;

public class WrapTestOne {
	public static void main(String[] args) {
		// 装箱：把基本数据类型转换成包装类
		// 1. 自动装箱
		int t1 = 2;
		Integer t2 = t1;
		// 2. 手动装箱
		Integer t3 = Integer.valueOf(t1);
		
		// 测试
		System.out.println("int类型变量t1="+t1);
		System.out.println("Integer类型对象t2="+t2);
		System.out.println("Integer类型对象t3="+t3);
	}
}
